    HexRadius 2 div neg	HexRadius Root3 mul 2 div neg rlineto
    HexRadius neg 		0 rlineto
    HexRadius 2 div neg	HexRadius Root3 mul 2 div rlineto
    closepath
    0 setlinewidth
    shade setgray
    fill

    i j Hexagon
} def

