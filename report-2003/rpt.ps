        0 1 DimX 1 sub
        {
            /i exch def
            i j Hexagon
        } for
    } for

    % draw horizontal edges
    0 1 DimX 1 sub
    {
        /i exch def
        i 0 RhombusEdge
        i DimY 1 sub RhombusEdge
    } for

    % draw vertical edges
    0 1 DimY 1 sub
    {
        /j exch def
        0 j RhombusEdge
        DimX 1 sub j RhombusEdge
    } for

    LabelAxes
    DrawGoals
} def

/RhombusEdge
{
    % function call: i j RhombusEdge
    /j exch def
    /i exch def

    newpath
    RhombusEdgeWidth setlinewidth
    0 setgray

    i 0 eq
    {
