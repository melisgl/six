    % Fill shape
    0 setlinewidth
    1 setgray
    newpath
    i j HexX i j HexY CircleRadius 0 360 arc
    fill

    % Draw outline
    0 setgray
    CircleLineWidth setlinewidth
    newpath
    i j HexX i j HexY CircleRadius 0 360 arc
